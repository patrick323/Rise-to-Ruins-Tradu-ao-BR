# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

timeOfDay:
  comment: "Time Module - Time of Day"
  javaClass: "rtr.TimeModule.java"
  
  night:
    text: "Noite"
  dawn:
    text: "Nascer do Sol"
  morning:
    text: "Manhã"
  midday:
    text: "Meio-Dia"
  evening:
    text: "Tarde"
  dusk:
    text: "Pôr do Sol"
  
