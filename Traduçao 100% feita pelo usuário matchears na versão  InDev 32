# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
# Object Module
# Object Overlay Messages
# rtr.objects.ObjectBase.java
# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

objectOverlay:
  comment: "Object Module - Object Overlay Messages"
  javaClass: "rtr.objects.ObjectBase.java" 
  
  requiredBuilding:
    text: "$RED1Requer~{buildingName}"
  requiredBuildingAdditional:
    comment: "This is used to join a list of required buildings, if more than one will meet the objects requirements."
    text: "$WHI0 ou $RED1{buildingName}"
  
  farms:
    tooCold:
      text: "$RED1Plantas não crescem neste $BLU0frio$RED1!"
    cropsDying:
      text: "$RED1A safra está morrendo!"
    poorlyTended:
      text: "$YEL1Mal manejado."
    poorlyWatered:
      text: "$YEL1Mal regado."  
  
  lowEnergy:
    text: "$YEL1Energia baixa!"
  noWorkers:
    text: "$YEL1Sem trabalhadores!"
  understaffed:
    text: "$YEL1Vaga vazia!"
  noProduction:
    text: "$YEL1Sem produção!"
  noSpawningResources:
    comment: "This is almost exclusively used for combobulators to warn the player no usable materials are nearby"
    text: "$YEL1Sem {resourceName} por perto!"
  
  noResources:
    comment: "This is for when there's no resources available to build a building under construction."
    text: "$YEL1Sem {resourceName}!"
    
