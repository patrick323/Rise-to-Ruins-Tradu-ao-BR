# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
# Mob Job Module
# Names and Descriptions
# rtr.mobs.jobs.*
# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

mobJob:
  comment: "Mob Job Module - Names and Descriptions"
  javaClass: "rtr.mobs.jobs.*"
  
  unassigned:
    single:
      text: "Desempregado"
    plural:
      text: "Desempregados"
    description:
      text: "Um vagabundo que não faz nada."

  builder:
    single:
      text: "Construtor"
    plural:
      text: "Construtores"
    description:
      text: "Um emprego importante na sua aldeia, construtores vão construir e reparar estruturas. Eles podem coletar materiais, mas só o farão quando precisam deles para construir algo e não tem nenhum disponível."

  carpenter:
    single:
      text: "Carpinteiro"
    plural:
      text: "Carpinteiros"
    description:
      text: "Transforma madeira em madeira refinada para estruturas avançadas."

  crystiller:
    single:
      text: "Cristrilador"
    plural:
      text: "Cristiladores"
    description:
      text: "Transforma cristais em crilítio, um combustível para coletores de essência e outras estruturas"

  crystalHarvester:
    single:
      text: "Coletor de Cristal"
    plural:
      text: "Coletores de Cristais"
    description:
      text: "Coleta cristais brutos no mapa e os estoca na aldeia. Eles continuarão coletando independente das necessidades da aldeia."

  farmer:
    single:
      text: "Fazendeiro"
    plural:
      text: "Fazendeiros"
    description:
      text: "Coleta comida selvagem, a planta em fazendas e as mantêm bem cuidadas e regadas para encorajar seu crescimento. Quanto mais fazendeiros, mais rápido suas fazendas produzirão comida."

  fletcher:
    single:
      text: "Flecheiro"
    plural:
      text: "Flecheiros"
    description:
      text: "Transforma madeira em flechas utilizadas por suas torres."

  lumberjack:
    single:
      text: "Lenhador"
    plural:
