# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #
# Console
# Console Panel and Banner Messages
# rtr.console.Console.java
# # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # # #

console:
  comment: "Console - Console Panel and Banner Messages"
  javaClass: "rtr.console.Console.java"
  
  panel:
    coitus:
      text: "{mobName} $WHI0acasalou com {mateMobName} $WHI0!"
    isPregnant:
      text: "{mobName} $PIN1está grávida!"
    object:
      destroyed:
        text: "{buildingName} $RED1foi destruído!"
      repaired:
        text: "{buildingName} $GRE1foi reparado."
      completed:
        text: "{buildingName} $WHI0foi construído."
      dismantled:
        text: "{buildingName} $YEL1foi demolido."
    autosave:
      text: "$GRE1Jogo salvo $WHI0-$GRE2 {time}"
    save:
      text: "$GRE1Jogo salvo, em $WHI0-$GRE2 {time}"
    fullMapScreenshot:
      text: "Captura completa do mapa salva em $GRE1{filePath}"
    screenshot:
      text: "Captura salva em $GRE1{filePath}"
      
  banner:
    time:
      newDayBanner:
        newDayBannerHeader:
          text: "{season}, dia {currentDay}"
        nextSeasonArrivesTomorrow:
          text: "Amanhã será {season}!"
        daysUntilNextSeason:
          text: "Em {days} dias, será {season}."
        monstersArriveTomorrow:
          text: "$RED0Monstros $WHI0 chegarão amanhã!" 
        daysUntilMonsters:
          text: "$RED0Monsters $WHI0 chegarão em {days} dias!"
      
      feelsOminous:
        header:
          yellow:
            text: "$YEL1O clima está tenso..."
          red:
            text: "$RED1O clima está tenso..."
        subtitle:
          text: "O que está acontecendo?"
      
      anEclipse:
        header:
          text: "$YEL1Um Eclipse!"
        subtitle:
          text: "Os monstros estão furiosos!"
          
      duskArrived:
        header:
          text: "$GRE0O sol se põe..."
        subtitle:
          peaceful:
            comment: "Monstros não irão aparecer, apesar do seu modo de jogo."
            text: "Tudo está calmo?"
          restless:
            comment: "Monstros estão começando a aparecer."
            text: "Os monstros parecem inquietos."
          alwaysPeaceful:
            comment: "Legenda do modo pacífico, onde nenhum monstro aparece."
            text: "Tudo está calmo."

      nightArrived:
        header:
          green:
            text: "$GRE0Anoiteceu..."
          blue:
            text: "$BLU0Anoiteceu..."
        subtitle:
          fullMoon:
            text: "A lua está cheia, e os monstros estão calmos."
          enraged:
            text: "Os monstros estão agitados!"
          calm:
            text: "Tudo está calmo."
      
      bloodMoon: 
        header:
          text: "$RED1O luar escarlate brilha!"
        subtitle:
          text: "Os monstros estão sedentos!"
      
      newSeason: 
        header:
          text: "É {season}!"
        subtitle:
          text: "$WHI0 Boa sorte!"
          
    migration:
      migrantsArrived:
        header:
          text: "$GRE1{amount}$WHI0 nômades chegaram!"
        subtitle:
          text: "Eles vem de $YEL1{regionName}$WHI0!"
    mob:
      born:
        subtitle:
          text: "nasceu!"
      hired:
        subtitle:
          text: "foi empregado/a!"
      grownUp:
        subtitle:
          text: "cresceu!"
      elder:
        subtitle:
          text: "se tornou um ancião!"
      migrantPrepared:
        subtitle:
          text: "se preparou para sair da aldeia!"
      nomadJoined:
        subtitle:
          text: "se mudou para a aldeia!"
      wildDoggoDomesticate:
        subtitle:
          text: "o cachorro se juntou à aldeia!"
          
    mobAI:
      childBirthDeath:
        header:
          text: "O bebê de {mobName} não sobrevivou ao parto!"
        subtitle:
          text: "Que pena."
      turnedIntoZombie:
        header:
          text: "A praga transformou o cadáver de {mobName}$WHI0 em um $RED1{mobType}$WHI0!"
        subtitle:
          text: "Lá se vai o descanso em paz."
      death:
        title:
          comment: "Some mobs, like the doggo, need their title inputted into the death messages. This string will be inputted where {mobTitle} is in all death strings under the death header category."
          text: "$WHI0 o {mobType}"
        header:
          frozen:
            text: "{mobName}{mobTitle}$RED1 morreu de frio!"
          byMob:
            text: "{mobName}{mobTitle}$WHI0 foi morto por um {killerMob}$WHI0!"
          byBuilding:
            text: "{mobName}{mobTitle}$WHI0 foi morto por uma {buildingName}$WHI0!"
          byUnknown:
            text: "{mobName}{mobTitle}$WHI0 morreu!"
          crashing:
            text: "{mobName}{mobTitle}$RED1 morreu esmagado!"
          dehydration:
            text: "{mobName}{mobTitle}$RED1 morreu de sede!"
          blight:
            text: "{mobName}{mobTitle}$RED1 morreu de infecção!"
          dropped:
            text: "{mobName}{mobTitle}$RED1 caiu de muito alto, e morreu no impacto!"
          fire:
            text: "{mobName}{mobTitle}$RED1 queimou até morte!"
